<?xml version="1.0" encoding="UTF-8"?>
<plugin name="webpage">
	<actions>
		<action name="webpage" id="webpage" ui="webpageui.qml" image="web-design.svg"/>
		
		<action name="Mozilla Color Picker" id="mozilla-colorpicker" 
			ui_width="1070" ui_height="880" image="color-wheel.png">
			<adopt id="webpage">
				<arguments>
					<arg name="url">https://mdn.mozillademos.org/en-US/docs/Web/CSS/CSS_Colors/Color_picker_tool$samples/ColorPicker_Tool?revision=1001473</arg>
				</arguments>
			</adopt>
		</action>
	</actions>
</plugin>
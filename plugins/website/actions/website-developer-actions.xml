			<link actionid="website-unrealengine" keyword="UnrealEngine"/>
			<link actionid="website-cocos2dx" keyword="cocos2dx"/>
			<link actionid="website-spring" keyword="SpringFramework"/>
		</links>
	</action>
	
	<!-- QT -->
	<action name="QT" id="website-qt">
		<do>
			<runaction condition="'input_text' != ''" id="website">
				<arguments>
					<arg name="input_text" value="http://doc.qt.io/qt-5/search-results.html?q={{input_text}}"/>
				</arguments>
			</runaction> 
			<runaction condition="'input_text' == ''" id="website">
				<arguments>
					<arg name="input_text" value="http://doc.qt.io/"/>
				</arguments>
			</runaction>
		</do>
	</action>
	

<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<!-- developers -->
	<action name="Developer Websites" id="website-developers">
		<do>
		</do>
		<links>
			<link actionid="website-qt" keyword="qt"/>
			<link actionid="website-boost" keyword="boost"/>
			<link actionid="website-cppreference" keyword="CppReference"/>
			<link actionid="website-mdn" keyword="mdn"/>
			<link actionid="website-w3schools" keyword="w3schools"/>
			<link actionid="website-jquery" keyword="jquery"/>
			<link actionid="website-bootstrap" keyword="BootStrap"/>
			<link actionid="website-angularjs" keyword="aNGularjs"/>
			<link actionid="website-react" keyword="react"/>
			<link actionid="website-msdn" keyword="msdn"/>
			<link actionid="website-stackoverflow" keyword="StackOverflow"/>
			<link actionid="website-github" keyword="GitHub"/>
